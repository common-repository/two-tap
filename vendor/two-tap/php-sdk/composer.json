{
    "name": "two-tap/php-sdk",
    "description": "The Two Tap SDK for PHP provides a native interface to the Two Tap API",
    "type": "library",
    "version": "1.0.6",
    "license": "MIT",
    "authors": [
        {
            "name": "TwoTap",
            "email": "hello@twotap.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "TwoTap\\": "src/TwoTap/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoTap\\Tests\\": "tests/"
        }
    }
}
